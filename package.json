{
  "name": "sangari-keyviewer",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "electron": "11.0.0-beta.11",
    "electron-builder": "^22.7.0",
    "electronmon": "^1.1.2",
    "node-abi": "^2.26.0"
  },
  "dependencies": {
    "iohook": "0.9.0",
    "lodash": "^4.17.20"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "4.0.0"
  },
  "iohook": {
    "targets": [
      "node-83",
      "electron-85"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "compression": "store",
    "productName": "Sangari-Keyviewer",
    "appId": "com.github.qwreey75.sangari_Keyviewer",
    "win": {
      "icon": "assets/icons/win/icon.ico",
      "target": [
        "portable",
        "zip",
        "nsis"
      ]
    },
    "asar": false,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "./app/",
      "app": "."
    },
    "mac": {
      "icon": "./assets/icons/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "icon": "./assets/icons/png/64x64.png",
      "target": [
        "appimage"
      ],
      "category": "Utility"
    }
  }
}
